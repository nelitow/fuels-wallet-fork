{
  "$schema": "https://unpkg.com/@changesets/config@2.0.0/schema.json",
  "privatePackages": {
    "version": true,
    "tag": true
  },
  "changelog": [
    "@changesets/changelog-github",
    {
      "repo": "nelitow/fuels-wallet-fork"
    }
  ],
  "commit": false,
  "fixed": [["@nelitow-fuel/*"]],
  "linked": [],
  "access": "public",
  "baseBranch": "master",
  "updateInternalDependencies": "patch",
  "ignore": ["@nelitow-fuel/docs", "@nelitow-fuel/e2e-contract-tests"],
  "snapshot": {
    "useCalculatedVersion": true,
    "prereleaseTemplate": "{tag}-{commit}"
  }
}
